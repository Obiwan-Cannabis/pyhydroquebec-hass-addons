{
"name": "pyhydroquebec",
"version": "master",
"slug": "pyhydroquebec",
"description": "Get your consumption data from your HydroQuebec account inside a hass addon",
"url":"https://github.com/titilambert/pyhydroquebec",
"arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
"startup": "application",
"boot": "auto",
"map": [
  "config:rw"
  ],
"services": [
  "mqtt:need"
  ],
"environment": {
  "MQTT_USERNAME": "ha",
  "MQTT_PASSWORD": "iotq1w2e3",
  "MQTT_HOST": "10.10.1.181",
  "MQTT_PORT": "1883",
  "PYHQ_OUTPUT": "MQTT",
  "CONFIG": "/data/options.json"
},
"options": {
  "timeout": "30",
  "frequency": "8640",
  "USERNAME": "USERNAME@EMAIL",
  "PASSWORD": "PASSWORD",
  "CONTRACT": "CONTRACT_ID",
  "MQTT_USERNAME": "MQTTUSER",
  "MQTT_PASSWORD": "MQTTPASSWORD",
  "MQTT_HOST": "FQDN/IP",
  "MQTT_PORT": "MQTTPORT",
  "PYHQ_OUTPUT": "MQTT"
},
"schema": {
  "timeout": "int(0,999999999)?",
  "frequency": "int",
  "USERNAME": "str",
  "PASSWORD": "str",
  "CONTRACT": "str",
  "MQTT_USERNAME": "str",
  "MQTT_PASSWORD": "str",
  "MQTT_HOST": "str",
  "MQTT_PORT": "int",
  "PYHQ_OUTPUT": "str"
},
"image": "registry.gitlab.com/ttblt-hass/pyhydroquebec/mqtt"
}