{
"name": "pyhydroquebec Add-on",
"version": "0.024",
"slug": "pyhydroquebec",
"description": "Get your consumption data from your HydroQuebec account inside this hass addon",
"url":"https://github.com/titilambert/pyhydroquebec",
"arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
"startup": "application",
"boot": "auto",
"map": [
  "config:rw"
  ],
"services": [
  "mqtt:need"
  ],
"options": {
  "PYHQ_USER":  "HQ account user",
  "PYHQ_PASSWORD": "HQ account password",
  "PYHQ_OUTPUT": "MQTT",
  "PYHQ_CONTRACT": "HQ contract number",
  "mqtt_username": "MQTTUSER",
  "mqtt_password": "MQTTPASSWORD",
  "mqtt_host": "FQDN/IP",
  "mqtt_port": "1883",
  "pyhq_output": "MQTT",
  "pyhqconfig": "/config/pyhq-config.yaml"

},
"schema": {
  "PYHQ_USER": "str",
  "PYHQ_PASSWORD": "str",
  "PYHQ_OUTPUT": "str",
  "PYHQ_CONTRACT": "str",
  "mqtt_username": "str",
  "mqtt_password": "str",
  "mqtt_host": "str",
  "mqtt_port": "int",
  "pyhq_output": "str",
  "pyhqconfig": "str"
},
"image": "registry.hub.docker.com/arsenick/pyhydroquebechassaddons"
}