{
"name": "pyhydroquebec",
"version": "0.1",
"slug": "pyhydroquebec",
"description": "Get your consumption data from your HydroQuebec account inside a hass addon",
"url":"https://github.com/titilambert/pyhydroquebec",
"arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
"startup": "application",
"boot": "auto",
"options": {
    "timeout": "30",
    "frequency": "8640",
    "accounts":{
        "username": "USERNAME@EMAIL",
        "password": "PASSWORD",
        "contracts":{
            "id": "CONTRACT_ID"
        }
    },
    "MQTT_USERNAME": "MQTTUSER",
    "MQTT_PASSWORD": "MQTTPASSWORD",
    "MQTT_HOST": "FQDN/IP",
    "MQTT_PORT": "MQTTPORT",
    "CONFIG": "config.yaml",
    "PYHQ_OUTPUT": "MQTT"
},
"schema": {
    "timeout": "int(0,999999999)?",
    "frequency": "int",
    "accounts":{
        "username": "str",
        "password": "str",
        "contracts":{
            "id": "str"
        }
    },
    "MQTT_USERNAME": "str",
    "MQTT_PASSWORD": "str",
    "MQTT_HOST": "str",
    "MQTT_PORT": "int",
    "CONFIG": "str",
    "PYHQ_OUTPUT": "str"
}
}